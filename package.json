{
  "name": "typescript-a2s",
  "version": "1.0.0",
  "description": "TypeScript library to query Source and GoldSource servers using Valve's Server Query Protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "example": "tsx examples/basic-usage.ts"
  },
  "keywords": [
    "a2s",
    "valve",
    "source",
    "goldsource",
    "server",
    "query",
    "protocol",
    "typescript"
  ],
  "author": "Generated from python-a2s",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.12",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "tsx": "^4.20.5",
    "typescript": "^5.3.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
